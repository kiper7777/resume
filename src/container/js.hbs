<div>
  <ul>
    <div><h2>{{name}}</h2></div>
    <br />
    <div>{{description}}</div>

    <hr />

    <div><mark><strong>History</strong></mark></div>
    <br />
    <ul>
      {{#with history}}
        <li>
          <div>{{year}}</div>
          <div>{{founder}}</div>
        </li>
      {{/with}}
    </ul>
    <br />
    <br />
    <div><mark><strong>Types</strong></mark></div>
    <br />
    <div>
      <ul>
        {{#each types}}

          <span
            class='badge text-bg-primary'
          >{{this}}</span>

        {{/each}}
      </ul>
    </div>
    <br />
    <br />
    <div><mark><strong>Syntax</strong></mark></div>
    <br />
    <div>
      <ul>
        {{#with syntax}}
          <li>
            <span
              class='badge text-bg-warning'
            >Variables</span>
            {{#each variables}}
              ({{this}})
            {{/each}}
          </li>
          <br />
          <li>
            <span
              class='badge text-bg-warning'
            >Functions</span>
            {{#each functions}}
              ({{this}})
            {{/each}}
          </li>
          <br />
          <li>
            <span
              class='badge text-bg-warning'
            >Conditionals</span>
            {{#each conditionals}}
              ({{this}})
            {{/each}}
          </li>
          <br />
          <li>
            <span class='badge text-bg-warning'>Loops</span>

            {{#each loops}}
              ({{this}})
            {{/each}}
          </li>
          <br />
          <li>
            <span
              class='badge text-bg-warning'
            >Classes</span>

            {{#each classes}}
              ({{this}})
            {{/each}}
          </li>
        {{/with}}
      </ul>
    </div>
    <br />
    <br />
    <div><mark><strong>Features</strong></mark></div>
    <br />

    <p class='d-inline-flex gap-1'>
      <a
        class='btn btn-primary'
        data-bs-toggle='collapse'
        href='#collapseExample'
        role='button'
        aria-expanded='false'
        aria-controls='collapseExample'
      >
        Features - Link with href
      </a>
      <button
        class='btn btn-primary position-relative'
        type='button'
        data-bs-toggle='collapse'
        data-bs-target='#collapseExample'
        aria-expanded='false'
        aria-controls='collapseExample'
      >
        Features - Button with data-bs-target
        <span
          class='position-absolute top-0 start-100 translate-middle badge rounded-pill bg-danger'
        >
          Features

        </span>
      </button>
    </p>
    <div class='collapse' id='collapseExample'>
      <div class='card card-body'>
        <div>
          <ul>
            {{#each features}}
              <li>
                {{this}}
              </li>
            {{/each}}
          </ul>
        </div>
      </div>
    </div>
    <br />
    <br />
    <br />
    <div><mark><strong>Features</strong></mark></div>
    <br />
    <div>
      <ul>
        {{#each features}}
          <li>
            {{this}}
          </li>
        {{/each}}
      </ul>
    </div>
    <br />
    <br />
    <div><mark><strong>Libraries</strong></mark></div>
    <br />
    <div>
      <ul>
        {{#each libraries}}
          <span class='badge text-bg-dark'>{{this}}</span>
        {{/each}}
      </ul>
    </div>
    <br />
    <br />
    <div><mark><strong>Tools</strong></mark></div>
    <br />
    <div>
      <ul>
        {{#each tools}}
          <span
            class='badge text-bg-success'
          >{{this}}</span>
        {{/each}}
      </ul>
    </div>
    <br />
    <br />
    <div><mark><strong>Community</strong></mark></div>
    <br />
    <div>
      <ul>
        {{#each community}}
          <li>{{name}} ({{type}})</li>
        {{/each}}
      </ul>
    </div>
    <br />

  </ul>
</div>